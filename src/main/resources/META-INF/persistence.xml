<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
 http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">
	<persistence-unit name="ursulaGIS" transaction-type="JTA">
	<jta-data-source>jdbc/sample</jta-data-source>
<!-- 		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider> -->
<!-- 		<exclude-unlisted-classes>false</exclude-unlisted-classes> -->
		<properties>
<!-- 			<property name="ssl" value="false" /> -->
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/UrsulaGIS_OLD" />
            
            <property name="javax.persistence.jdbc.user" value="ursulaUser" />
            <property name="javax.persistence.jdbc.password" value="ursulaPass" />
		</properties>
	</persistence-unit>
</persistence>